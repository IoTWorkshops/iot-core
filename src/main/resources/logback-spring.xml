<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true">
	<!--Default Console Appender: DO NOT DELETE! Disable from application properties, set 'logging.console.default.active' to false-->
	<appender name="defaultConsoleAppender" class="ch.qos.logback.core.ConsoleAppender">
		<withJansi>true</withJansi>
		<encoder>
			<charset>UTF-8</charset>
			<Pattern>%d %-4relative [%thread] %-5level %logger{100} - %msg%n</Pattern>
		</encoder>
	</appender>
	<!--Default Console Appender: DO NOT DELETE! Disable from application properties, set 'logging.console.default.active' to false-->
	<root level="INFO">
		<appender-ref ref="defaultConsoleAppender" />
	</root>
</configuration>
